{
	"name": "miniprogram_for_music",
	"version": "1.0.0",
	"private": true,
	"description": "miniprogram for music",
	"templateInfo": {
		"name": "redux",
		"typescript": true,
		"css": "sass"
	},
	"author": "jetair",
	"license": "MIT",
	"scripts": {
		"build:weapp": "taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:rn": "taro build --type rn",
		"build:qq": "taro build --type qq",
		"build:quickapp": "taro build --type quickapp",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"dev:qq": "npm run build:qq -- --watch",
		"dev:quickapp": "npm run build:quickapp -- --watch",
		"eslint": "eslint src --ext .ts,.tsx",
		"lint": "eslint ./src --fix",
		"lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
		"lint:style-fix": "stylelint \"src/**/*.scss\" --syntax scss --fix",
		"temp": "node ./scripts/temp.js",
		"commit": "git-cz"
	},
	"stylelint": {
		"extends": "stylelint-config-standard",
		"plugins": [
			"stylelint-scss"
		],
		"rules": {
			"unit-no-unknown": [
				true,
				{
					"ignoreUnits": [
						"rpx"
					]
				}
			],
			"media-feature-name-no-unknown": [
				true,
				{
					"ignoreMediaFeatureNames": "min-device-pixel-ratio"
				}
			],
			"selector-type-no-unknown": [
				true,
				{
					"ignore": [
						"custom-elements",
						"default-namespace"
					]
				}
			],
			"font-family-no-missing-generic-family-keyword": null,
			"declaration-colon-newline-after": null,
			"at-rule-no-unknown": null,
			"color-hex-case": null,
			"block-closing-brace-newline-after": null,
			"at-rule-empty-line-before": null,
			"number-no-trailing-zeros": null,
			"no-empty-source": null,
			"unit-case": null,
			"scss/at-rule-no-unknown": true
		}
	},
	"dependencies": {
		"@tarojs/async-await": "2.2.10",
		"@tarojs/components": "2.2.10",
		"@tarojs/components-qa": "2.2.10",
		"@tarojs/redux": "2.2.10",
		"@tarojs/redux-h5": "2.2.10",
		"@tarojs/router": "2.2.10",
		"@tarojs/taro": "2.2.10",
		"@tarojs/taro-alipay": "2.2.10",
		"@tarojs/taro-h5": "2.2.10",
		"@tarojs/taro-qq": "2.2.10",
		"@tarojs/taro-quickapp": "2.2.10",
		"@tarojs/taro-rn": "2.2.10",
		"@tarojs/taro-swan": "2.2.10",
		"@tarojs/taro-tt": "2.2.10",
		"@tarojs/taro-weapp": "2.2.10",
		"classnames": "^2.2.6",
		"dva-model-extend": "^0.1.2",
		"nerv-devtools": "^1.5.7",
		"nervjs": "^1.5.7",
		"prop-types": "^15.7.2",
		"qs": "^6.9.3",
		"redux": "^4.0.0",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"taro-ui": "^2.2.4"
	},
	"devDependencies": {
		"@tarojs/cli": "2.2.10",
		"@tarojs/mini-runner": "2.2.10",
		"@tarojs/plugin-babel": "2.2.10",
		"@tarojs/plugin-csso": "2.2.10",
		"@tarojs/plugin-sass": "2.2.10",
		"@tarojs/plugin-uglifyjs": "^2.2.7",
		"@tarojs/webpack-runner": "2.2.10",
		"@types/classnames": "^2.2.9",
		"@types/react": "^16.4.8",
		"@types/redux-logger": "^3.0.7",
		"@types/webpack-env": "^1.13.6",
		"@typescript-eslint/eslint-plugin": "^2.13.0",
		"@typescript-eslint/parser": "^2.13.0",
		"babel-eslint": "^8.2.3",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-jsx-stylesheet": "^0.6.5",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"commitizen": "^4.1.2",
		"cz-conventional-changelog": "^3.2.0",
		"dva-core": "^2.0.2",
		"dva-loading": "^3.0.20",
		"eslint": "^5.16.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-taro": "2.2.10",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-react-hooks": "^1.6.1",
		"eslint-plugin-taro": "2.2.10",
		"husky": "^4.2.5",
		"stylelint": "^13.2.0",
		"stylelint-config-standard": "^20.0.0",
		"stylelint-config-taro-rn": "2.2.10",
		"stylelint-scss": "^3.14.2",
		"stylelint-taro-rn": "2.2.10",
		"typescript": "^3.0.1"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-conventional-changelog"
		}
  },
  "husky": {
    "hooks": {
      "commit-msg": "node scripts/verify-commit-msg.js"
    }
  }
}
